CREATE TABLE alumno(
    id int auto_increment,
    nombre varchar(100),
    apellidos varchar(100),
    email varchar(100),
    edad int(3),
    PRIMARY KEY(id)
);

INSERT INTO alumno(nombre,apellidos,email,edad) 
